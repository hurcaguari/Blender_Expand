{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Clean Directories",
            "type": "shell",
            "command": "powershell",
            "args": [
                "-NoProfile",
                "-ExecutionPolicy",
                "Bypass",
                "-Command",
                "Remove-Item -Recurse -Force data_zip, expand_data, api_json/*, data/, *.tar"
            ],
            "problemMatcher": []
        },
        {
            "label": "Construct Docker Image",
            "type": "shell",
            "command": "./docker_build.bat",
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}